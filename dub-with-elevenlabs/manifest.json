{
  "manifest_version": 3,
  "name": "Dub with ElevenLabs",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["*://*.youtube.com/*", "*://*.elevenlabs.io/*"],
  "background": {
    "service_worker": "background.js",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    } 
    ]
}
